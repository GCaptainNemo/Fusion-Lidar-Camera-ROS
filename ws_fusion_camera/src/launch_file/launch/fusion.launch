<launch>
	<node pkg="color_pc" type="color_pc" name="color_pc" args="false" output="screen">
		<rosparam file="$(find launch_file)/config/param.yaml" command="load"/>	
	</node>
	<node pkg="rviz" type="rviz" name="rviz_color_pc" args="-d $(find launch_file)/config/show.rviz" required="true" />
</launch>
